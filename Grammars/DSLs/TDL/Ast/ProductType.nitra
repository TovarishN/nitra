using DotNet;
using DotNet.AstUtils;

using Nitra;
using Nitra.Declarations;

namespace Tdl
{
  declaration ProductType : NamespaceMember, Container
  {
    symbol
    {
      Scope     = MemberTable;
      Kind      = "product type";
      SpanClass = TdlLang.ProductTypeSpanClass;
    }

    Fields.ContainingTable = Symbol.MemberTable;
    Fields.Scope = Scope;
    
    Fields : Field*;
  }
}
