language TdlLang : DotNetLang
{
  span class StringEx         { ForegroundColor=Maroon;     }

  extension = .tdl;

  company     = NitraTeam;
  description = Nitra driven Tdl plugin for Visual Studio;
  authors     = Vlad Chistyakov;
  copyright   = NitraTeam;

  span class Platform   { ForegroundColor=LightSeaGreen; }
  span class Deployment { ForegroundColor=DarkCyan; }

  syntax module Tdl.Grammar start rule Module;
}
