using Nitra;
using Nitra.Runtime.Binding;
using Nemerle.Collections;

namespace CSharp
{
  abstract ast TypeParameterConstraint
  {
  stage 1:
    in Scope  : Scope;

    | Ctor
    | Class
    | Struct
    | Base
      {
      stage 1:
        out Symbol : ISymbol2 = Type.Symbol;

        Type.Scope = Scope;

        Type : QualifiedReference;
      }
  }
}
