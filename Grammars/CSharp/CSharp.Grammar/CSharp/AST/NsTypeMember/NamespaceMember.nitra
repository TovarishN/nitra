using Nitra;
using Nitra.Runtime.Binding;

using System.Linq;

namespace CSharp
{
  abstract declaration NamespaceMember : BindableAst
  {
    symbol
    {
      in Parent : NamespaceSymbol;
    }

    in Parent  : NamespaceSymbol;

    Symbol.Parent   = Parent;
    ContainingScope = Parent.MemberScope;
  }
}
