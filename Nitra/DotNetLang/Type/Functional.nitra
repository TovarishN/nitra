using Nitra.Declarations;

namespace DotNet
{
  abstract declaration Functional : Parameterizable
  {
    symbol
    {
      in ReturnType : TypeSymbol;
      in Type       : FunctionTypeSymbol = FunctionTypeSymbol();

      Type.Return     = ReturnType;
      Type.Parameters = AstUtils.MapParametersToTypes(Parameters, ReturnType);
    }
  }
}
